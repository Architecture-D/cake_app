
<h2>新規会員登録</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    姓<%= f.text_field :last_name, autofocus: true %>

    名<%= f.text_field :first_name, autofocus: true %>
  </div>
<br>
  <div class="field">
    セイ<%= f.text_field :last_name_kana, autofocus: true %>

    メイ<%= f.text_field :first_name_kana, autofocus: true %>
  </div>
<br>
  <div class="field">
    メール<%= f.email_field :email, autofocus: true %>
  </div>
<br>
  <div class="field">
    郵便番号<%= f.text_field :post_code, autofocus: true %>
  </div>
<br>
  <div class="field">
    住所<%= f.text_field :address, autofocus: true %>
  </div>
<br>
  <div class="field">
    電話番号<%= f.text_field :phone_number, autofocus: true %>
  </div>
<br>
  <div class="field">
    パスワード<%= f.password_field :password %>
  </div>
<br>
  <div class="field">
    確認用<%= f.password_field :password_confirmation %>
  </div>
<br>
  <div class="actions">
    <%= f.submit "新規登録",class:"btn btn-success"%>
  </div>
<% end %>

<%= render "devise/shared/links" %>

